/*分页*/
.page {
    background: #efefef;
    border: 1px solid #ccc;
    border-left: 0;
    border-right: 0;
    width: 100%;
    height: 30px;
    line-height: 30px;
    vertical-align: middle;
}

    .page .page_info {
        padding-left: 10px;
    }

    .page .pagenum {
        padding-right: 10px;
    }

.pagenum .page_select {
    border: 1px solid #ccc;
    height: 22px;
    line-height: 22px;
    vertical-align: middle;
}

.l_btn01, .l_btn02, .r_btn01, .r_btn02 {
    background: url(../images/pagination_icons.png) no-repeat;
    border: none;
}

.pagenum .l_btn, .pagenum .r_btn {
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
    display: inline-block;
    height: 18px;
    line-height: 18px;
    vertical-align: middle;
}

.l_btn01 {
    width: 8px;
    height: 10px;
    background-position: -4px -3px;
    margin: 0px 10px;
    float: left;
    margin-top: 4px;
}

.l_btn02 {
    width: 6px;
    height: 10px;
    background-position: -21px -3px;
    margin: 0px 10px 0 0;
    float: left;
    margin-top: 4px;
}

.r_btn01 {
    width: 6px;
    height: 10px;
    background-position: -37px -3px;
    margin: 0px 10px;
    float: left;
    margin-top: 4px;
}

.r_btn02 {
    width: 8px;
    height: 10px;
    background-position: -52px -3px;
    margin-right: 10px;
    float: left;
    margin-top: 4px;
}

.pagenum .disabled {
    filter: alpha(opacity=50);
    -moz-opacity: 0.5;
    -khtml-opacity: 0.5;
    opacity: 0.5;
}

.page .page_txt {
    border: 1px solid #ccc;
    width: 40px;
    height: 18px;
    text-align: center;
    margin: 0px 6px;
    line-height: 18px;
    vertical-align: middle;
}




