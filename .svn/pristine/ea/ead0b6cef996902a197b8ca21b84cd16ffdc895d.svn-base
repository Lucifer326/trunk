/*功能*/
.funciton, .funsearch {
    background: url(../images/function_bg.jpg) repeat-x #eeeff1;
    width: 100%;
    border-bottom: 1px solid #afafaf;
    overflow: hidden;
    padding: 3px 0px;
}

    .funciton ul li {
        float: left;
        cursor: pointer;
        text-align: center;
        line-height: 25px;
        vertical-align: middle;
        padding: 0px 11px;
        border: 1px solid #dfdfdf;
        margin: 0px 5px;
        background: #ebecec;
        display: inline-block;
    }

        .funciton ul li:hover {
            background: #e2e2e2;
            line-height: 25px;
            vertical-align: middle;
        }

        .funciton ul li a {
            float: left;
            display: block;
            cursor: pointer;
            color: #3a3a3a;
        }

    .funciton .icon_register a {
        background: url(../images/icon_register.png) no-repeat left;
        padding-left: 20px;
    }

    .funciton .icon_seeflow a {
        background: url(../images/icon_seeflow.png) no-repeat left;
        padding-left: 20px;
    }
    .funciton .icon_back a {
        background: url(../images/icon_back.png) no-repeat left;
        padding-left: 20px;
    }
    .funciton .icon_ok a {
        background: url(../images/ok.png) no-repeat left;
        padding-left: 20px;
    }
    /*查询*/
    .funsearch .search_list {
        line-height: 30px;
        vertical-align: middle;
        float: left;
    }

.search_list span {
    display: inline-block;
    margin-left: 10px;
}

.search_list .search_txt {
    width: 100px;
    height: 20px;
    border: 1px solid #ccc;
    line-height: 20px;
    vertical-align: middle;
    padding-left: 2px;
    display: inline-block;
}

.search_list .select_txt {
    border: 1px solid #ccc;
    padding-left: 2px;
    height: 22px;
    line-height: 22px;
    vertical-align: middle;
    display: inline-block;
}

.funsearch .search_bt {
    width: 71px;
    height: 22px;
    background: url(../images/search_bt.jpg) no-repeat 0 center;
    border: none;
    color: #3a3a3a;
    margin-left: 15px;
    float: left;
    margin-top: 6px;
    font-weight: bold;
    padding: 2px 0px 0px 10px;
}

    .funsearch .search_bt:hover {
        cursor: pointer;
        filter: alpha(opacity=70);
        -moz-opacity: 0.7;
        -khtml-opacity: 0.7;
        opacity: 0.7;
    }
